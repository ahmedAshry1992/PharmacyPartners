@model IEnumerable<PharmacyUI.Models.WebRequest.Response.Management.Cuctomer>
<table class="table table-hover " cellspacing="0" width="100%">
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="sel-customer" x-cusid="@item.id" x-cusname="@item.Name" x-cusphone="@item.phoneNumber" x-cusaddr="@item.address" x-points="@item.points">@item.Name</td>
            </tr>
        }
    </tbody>
</table>



      